import { all, AllEffect, select, takeLatest } from 'redux-saga/effects';
import { getType } from 'typesafe-actions';

import { defaultAction } from './actions';

function* doDefaultAction() {
  // See example in containers/HomePage/sagas.js
}

export default function* rootSaga(): IterableIterator<AllEffect> {
  yield all([takeLatest(getType(defaultAction), doDefaultAction)]);
}
