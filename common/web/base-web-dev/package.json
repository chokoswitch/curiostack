  "devDependencies": {
    "@types/html-webpack-plugin": "2.30.2",
    "@types/http-proxy-middleware": "0.17.4",
    "@types/koa-router": "7.0.27",
    "@types/webpack": "3.8.8",
    "fork-ts-checker-webpack-plugin": "0.4.0"
  }
