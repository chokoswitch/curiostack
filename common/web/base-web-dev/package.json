{
  "name": "@curiostack/base-web-dev",
  "version": "0.0.6",
  "description": "Curiostack's base dev dependencies for browser applications.",
  "main": "build/index.js",
  "bin": {
    "build-app": "./build/scripts/build-app.js",
    "dev-server": "./build/scripts/dev-server.js"
  },
  "repository": "curioswitch/curiostack",
  "author": "Choko <choko@curioswitch.org>",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn run build:clean",
    "build": "tsc --outDir ./build --noEmit false --target es2016",
    "build:clean": "rimraf ./build",
    "//": "Normally we would lint before build, but we are actually building the lint config here.",
    "postbuild": "yarn run lint",
    "prepublish": "yarn run build",
    "lint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "@curiostack/base-node-dev": "0.0.3",
    "@types/history": "4.6.2",
    "@types/intl": "1.2.0",
    "@types/prop-types": "15.5.2",
    "@types/react-dom": "16.0.4",
    "@types/react-loadable": "5.3.3",
    "@types/webpack-env": "1.13.5",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "brotli-webpack-plugin": "0.5.0",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "0.4.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.4",
    "http-proxy-middleware": "0.17.4",
    "image-webpack-loader": "4.1.0",
    "koa-proxies": "0.6.2",
    "koa-router": "7.4.0",
    "koa2-history-api-fallback": "0.0.5",
    "raw-loader": "0.5.1",
    "ts-loader": "4.0.1",
    "tsconfig-paths-webpack-plugin": "2.0.0",
    "tslint-config-airbnb": "5.7.0",
    "tslint-react": "3.5.1",
    "webapp-webpack-plugin": "1.1.0",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.10",
    "webpack-serve": "0.1.5",
    "zopfli-webpack-plugin": "0.1.0"
